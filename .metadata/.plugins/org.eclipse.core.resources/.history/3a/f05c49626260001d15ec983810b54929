package Unidad4Actividades;

public class Matrices4 {

	public static void main(String[] args) {
	int m[][]=new int[3][4];
	
	for (int i = 0; i < m.length; i++) {
		for (int j = 0; j < m[0].length; j++) {
			m[i][j]=(int)(1+Math.random()*25);
			System.out.print(m[i][j]);
		}
	}
	
	
	

	}

}
